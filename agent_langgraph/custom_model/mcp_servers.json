{
  "mcpServers": {
    "mcp-atlassian": {
      "autoApprove": [],
      "type": "stdio",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/etc/ssl/certs/ca-certificates.crt:/etc/ssl/certs/ca-certificates.crt:ro",
        "-e",
        "CONFLUENCE_URL",
        "-e",
        "CONFLUENCE_PERSONAL_TOKEN",
        "-e",
        "JIRA_URL",
        "-e",
        "JIRA_PERSONAL_TOKEN",
        "-e",
        "JIRA_SSL_VERIFY=false",
        "-e",
        "CONFLUENCE_SSL_VERIFY=false",
        "ghcr.io/sooperset/mcp-atlassian:latest"
      ],
      "env": {
        "CONFLUENCE_URL": "https://confluence.cec.lab.emc.com/",
        "CONFLUENCE_PERSONAL_TOKEN": "<INSERT_TOKEN_HERE>",
        "JIRA_URL": "https://jira.cec.lab.emc.com/",
        "JIRA_PERSONAL_TOKEN": "<INSERT_TOKEN_HERE>"
      }
    },
    "github": {
      "disabled": false,
      "type": "stdio",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/etc/ssl/certs/ca-certificates.crt:/etc/ssl/certs/ca-certificates.crt:ro",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "-e",
        "GITHUB_HOST=https://eos2git.cec.lab.emc.com",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "<INSERT_TOKEN_HERE>",
        "GITHUB_USERNAME": "kumarp94"
      }
    },
    "sonarqube": {
  "type": "stdio",
  "command": "docker",
  "args": [
    "run",
    "-i",
    "--rm",
    "-e", "SONARQUBE_URL",
    "-e", "SONARQUBE_TOKEN",
    "mcp/sonarqube-dell"
  ],
  "env": {
    "SONARQUBE_URL": "https://sonarqube1.cec.lab.emc.com",
    "SONARQUBE_TOKEN": "<INSERT_TOKEN_HERE>"
  }
}
  }
}
